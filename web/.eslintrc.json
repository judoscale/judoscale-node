{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "root": true,
    "rules": {
      "semi": [2, "never"],
      "comma-dangle": [2, "never"],
      "no-console": 0,
      "no-cond-assign": 0,
      "class-methods-use-this": 0,
      "max-len": [
        "error",
        {
          "code": 160
        }
      ]
    }
}
